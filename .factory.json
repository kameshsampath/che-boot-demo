{
  "v": "4.0",
  "name": "sbweb",
  "creator": {
    "name": "Kamesh Sampath",
    "email": "kamesh.sampath@hotmail.com"
  },
  "workspace": {
    "commands": [
      {
        "commandLine": "scl enable rh-maven33 'mvn install -f ${current.project.path}'",
        "name": "build",
        "type": "mvn",
        "attributes": {
          "goal": "Build",
          "previewUrl": ""
        }
      },
      {
        "commandLine": "scl enable rh-maven33 'mvn clean install -f ${current.project.path}'",
        "name": "clean build",
        "type": "mvn",
        "attributes": {
          "goal": "Build",
          "previewUrl": ""
        }
      },
      {
        "commandLine": "scl enable rh-maven33 'mvn compile spring-boot:run -f ${current.project.path}'",
        "name": "run",
        "type": "custom",
        "attributes": {
          "goal": "Run",
          "previewUrl": "http://${server.port.8080}"
        }
      },
      {
        "commandLine": "scl enable rh-maven33 'mvn spring-boot:run -Drun.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=y,address=5005\" -f ${current.project.path}'",
        "name": "debug",
        "type": "custom",
        "attributes": {
          "goal": "Debug",
          "previewUrl": "http://${server.port.8080}"
        }
      }
    ],
    "projects": [
      {
        "description": "A Spring Boot Web Project with Che Factory",
        "source": {
          "location": "https://github.com/kameshsampath/che-boot-demo",
          "type": "git",
          "parameters": {}
        },
        "mixins": [
          "git"
        ],
        "problems": [],
        "name": "che-boot-demo",
        "type": "maven",
        "path": "/che-boot-demo",
        "attributes": {
          "language": [
            "java"
          ]
        }
      }
    ],
    "defaultEnv": "default",
    "name": "spring-boot-web",
    "attributes": {},
    "environments": {
      "default": {
        "recipe": {
          "location": "rhche/spring-boot",
          "type": "dockerimage"
        },
        "machines": {
          "dev-machine": {
            "servers": {},
            "agents": [
              "org.eclipse.che.terminal",
              "org.eclipse.che.ws-agent",
              "org.eclipse.che.exec"
            ],
            "attributes": {
              "memoryLimitBytes": "2147483648"
            }
          }
        }
      }
    }
  },
  "policies": {
     "create": "perClick"
  },
  "ide":{
    "onProjectsLoaded": {
       "actions": [{
          "id": "openPomFile",
          "properties": {
             "file": "${current.project.path}/pom.xml"
          }
       }]
    }
  }
}
